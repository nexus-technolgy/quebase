{
  "name": "@nexustech/quebase-web",
  "version": "0.0.1",
  "description": "A Quasar-Vue-Firebase web front end boilerplate for quebase API",
  "productName": "quebase",
  "author": "Daniel Bryar <dbryar@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint:fix": "eslint --ext .js,.ts,.vue ./ --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "npm run lint:fix && quasar dev",
    "build": "npm run lint:fix && quasar build"
  },
  "dependencies": {
    "@nexustech/fingerprint": "^1.0.4",
    "@nexustech/logger": "^1.4.0",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "firebase": "^10.2.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.3.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
